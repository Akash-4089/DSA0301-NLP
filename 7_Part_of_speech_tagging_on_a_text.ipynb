      "source": [
        "import nltk\n",
        "text = \"The quick brown fox jumps over the lazy dog\"\n",
        "words = nltk.word_tokenize(text)\n",
        "pos_tags = nltk.pos_tag(words)\n",
        "for word, pos_tag in pos_tags:\n",
        "    print(f\"Word: {word}, POS Tag: {pos_tag}\")"
      ],
